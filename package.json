  "license": "GPL-3.0",
  "main": "src/app.js",
  "homepage": "https://favna.xyz/ribbon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Favna/Ribbon.git"
  },
  "bugs": {
    "url": "https://github.com/Favna/Ribbon/issues"
  },
  "scripts": {
    "docs": "jsdoc2md --template ./docs/template.hbs --files ./src/**/*.js | tee docs/index.md ../Ribbon.wiki/All-Commands.md > NUL",
    "jsondocs": "jsdoc --template ./node_modules/jsdoc-json --destination ../favna.xyz/public/dist/ribbondocs.json --recurse ./src/commands",
    "deploydocs": "yarn run docs && yarn run jsondocs && git commit -am documentation && git push && cd ../Ribbon.wiki/ && git commit -am documentation && git push && cd ../favna.xyz/ && git commit -am documentation && git push",
    "lintfix": "./node_modules/.bin/eslint --fix **/*.js",
    "lint": "./node_modules/.bin/eslint **/*.js",
    "start": "node src/app.js",
    "test": "yarn run lintfix && yarn lint && node src/test/test.js | tap-summary"
  },
  "keywords": [
    "bot",
    "Commando",
    "discord",
    "discord.js",
    "discord.js-commando",
    "Favna",
    "NodeJS"
  ],
