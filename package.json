{
  "name": "discord-ribbon",
  "version": "8.1.0",
  "description": "feature rich, modular Discord.JS-Commando server bot written in TypeScript",
  "author": "Favna <sharkie.jeroen@gmail.com> (https://favna.xyz/home)",
  "license": "GPL-3.0",
  "main": "src/app.ts",
  "homepage": "https://favna.xyz/ribbon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favna/ribbon.git"
  },
  "bugs": {
    "url": "https://github.com/favna/ribbon/issues"
  },
  "scripts": {
    "lint": "tslint -p . -c ./tslint.json ./src/**/*.ts ./src/commands/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "test": "yarn lint && mocha",
    "clean": "rimraf dist/",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "concurrently \"yarn copyfiles\" \"yarn replace\" && yarn uglify",
    "uglify": "uglifyjs-folder dist/ -yeo dist -x .js",
    "copyfiles": "copyfiles -u 1 ./src/data/fonts/* ./src/data/databases/* ./src/.env ./dist/",
    "replace": "replace 'typescript: true,' 'typescript: false,' dist/Ribbon.js && replace '.registerCommandsIn\\(\\{\\n\\s+dirname.+\\n\\s+filter.+\\n\\s+}\\);' '.registerCommandsIn(path.join(__dirname, \"commands\"));' dist/Ribbon.js",
    "docs": "yarn build --removeComments false && concurrently \"yarn docs:index\" \"yarn docs:wiki\" \"yarn docs:json\" && yarn clean",
    "docs:index": "jsdoc2md --template ./docs/template.hbs --files ./dist/commands/**/*.js > docs/index.md",
    "docs:wiki": "jsdoc2md --template ./docs/template.hbs --files ./dist/commands/**/*.js > ../wikiribbon/All-Commands.md",
    "docs:json": "jsdoc2md --json --files ./dist/commands/**/*.js > ../homesite/src/assets/docs/ribbon.json",
    "start": "cross-env NODE_ENV=development ts-node ./src/app.ts",
    "start:ribbon": "cross-env NODE_ENV=production ts-node ./src/app.ts",
    "serve": "pm2 start ./config/prod.yml --env production",
    "serve:test": "pm2 start ./config/dev.yml"
  },
  "keywords": [
    "bot",
    "Commando",
    "discord",
    "discord.js",
    "discord.js-commando",
    "Favna",
    "NodeJS",
    "TypeScript",
    "Ribbon"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@discordjs/uws": "^10.149.0",
    "@types/better-sqlite3": "^5.2.1",
    "@types/cheerio": "^0.22.10",
    "@types/common-tags": "^1.8.0",
    "@types/dotenv": "^6.1.0",
    "@types/fast-levenshtein": "^0.0.1",
    "@types/libsodium-wrappers": "^0.7.0",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/qrcode": "^1.3.0",
    "awesome-querystring": "^1.1.2",
    "better-sqlite3": "^5.2.1",
    "booru": "favna/booru",
    "cheerio": "^1.0.0-rc.2",
    "common-tags": "^1.8.0",
    "didyoumean2": "^2.0.0",
    "discord.js": "favna/discord.js#main",
    "discord.js-commando": "favna/commando#ribbon",
    "dotenv": "^6.1.0",
    "emoji-regex": "^7.0.3",
    "fast-levenshtein": "^2.0.6",
    "fuse.js": "favna/fuse",
    "jimp": "^0.6.0",
    "libsodium-wrappers": "^0.7.3",
    "moment": "favna/moment#fix/typing_declaration",
    "moment-duration-format": "^2.2.2",
    "nintendo-switch-eshop": "^1.1.4",
    "node-fetch": "^2.3.0",
    "node-opus": "^0.3.1",
    "qrcode": "^1.3.2",
    "slot-machine": "^2.1.0",
    "speedtest-net": "^1.4.3",
    "unescape-es6": "^2.0.7",
    "ytdl-core": "^0.28.3",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "replace": "^1.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "uglifyjs-folder": "^1.5.1"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./src",
    "test": "./test"
  }
}
